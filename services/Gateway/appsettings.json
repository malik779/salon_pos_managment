        "Transforms": [
          { "PathPattern": "/api/sync/{**catch-all}" }
        ]
      },

      "audit-route": {
        "ClusterId": "audit-cluster",
        "Match": { "Path": "/api/audit/{**catch-all}" },
        "Transforms": [
          { "PathPattern": "/api/audit/{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity-service:8080"
          }
        }
      },
      "branch-cluster": {

        "Transforms": [
          { "PathPattern": "/api/payments/{**catch-all}" }
        ]
      },

      "staff-route": {
        "ClusterId": "staff-cluster",
        "Match": { "Path": "/api/staff/{**catch-all}" },
        "Transforms": [
          { "PathPattern": "/api/staff/{**catch-all}" }
        ]
      },

      "client-route": {
        "ClusterId": "client-cluster",
